{
  "manifest_version": 2,

  "name": "Skroll new tab",
  "description": "This extension opens Skroll in new tab",
  "version": "1.0",

  "background" : {
    "scripts" : ["background.js"]
  },

  "browser_action": {
    "default_title": "My page",
    "default_icon": "icon.png"
  },

  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
